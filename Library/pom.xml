	</parent>
	<groupId>com.Libraries </groupId>
	<artifactId>Library </artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>Library </name>
	<description>project for Spring Boot</description>
	<url/>
	<licenses>
		<license/>
	</licenses>
	<developers>
		<developer/>
